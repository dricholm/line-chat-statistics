{
  "name": "line-chat-statistics",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ja": "ng serve --configuration=ja",
    "build": "ng build --prod",
    "build:ja": "ng build --configuration=production-ja",
    "deploy": "npm run deploy:build && npm run deploy:ngh",
    "deploy:build": "npm run deploy:build:en && npm run deploy:build:ja",
    "deploy:build:en": "ng build --prod --base-href=https://dricholm.github.io/line-chat-statistics/en/",
    "deploy:build:ja": "ng build --prod --base-href=https://dricholm.github.io/line-chat-statistics/ja/ --configuration=production-ja",
    "deploy:ngh": "ngh --no-silent",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "lint": "ng lint",
    "coverage": "jest --coverage",
    "extract": "ng xi18n --output-path=locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/localize": "~9.0.1",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@swimlane/ngx-charts": "^13.0.2",
    "bootstrap": "^4.4.1",
    "d3": "^5.11.0",
    "line-chat-parser": "^2.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@types/jest": "^25.1.2",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-preset-angular": "^8.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
}
