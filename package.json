{
  "name": "line-chat-statistics",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ja": "ng serve --configuration=ja",
    "build": "ng build --prod",
    "build:ja": "ng build --configuration=production-ja",
    "deploy": "npm run deploy:build && npm run deploy:ngh",
    "deploy:build": "npm run deploy:build:en && npm run deploy:build:ja",
    "deploy:build:en": "ng build --prod --base-href=https://dricholm.github.io/line-chat-statistics/en/",
    "deploy:build:ja": "ng build --prod --base-href=https://dricholm.github.io/line-chat-statistics/ja/ --configuration=production-ja",
    "deploy:ngh": "ngh --no-silent",
    "test": "ng test --watch",
    "test:ci": "ng test --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "coverage": "ng test --watch=false --code-coverage",
    "extract": "ng xi18n --output-path=locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@swimlane/ngx-charts": "^12.0.1",
    "angular-date-value-accessor": "0.0.2",
    "bootstrap": "^4.3.1",
    "d3": "^5.11.0",
    "line-chat-parser": "^1.3.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
