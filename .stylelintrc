{
  "extends": "stylelint-config-recommended",
  "plugins": ["stylelint-order"],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["extend"]
      }
    ],
    "block-no-empty": null,
    "order/properties-alphabetical-order": true,
    "string-quotes": "double"
  }
}
